{
  "name": "Octohelper",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "github helpers enhanced the activity page",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "homepage_url": "http://github.com/anasnakawa/chrome-github-avatars",
  "permissions": [
    "tabs",
    "*://github.com/"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
